{
  "brace-style": ["error", "1tbs", { "allowSingleLine": true }],
  "curly": ["error", "all"],
  "eqeqeq": ["error", "always"],
  "indent": ["error", 2, {"MemberExpression": 1}],
  "import/first": ["error"],
  "import/no-unresolved": ["error"],
  "import/no-useless-path-segments": ["error"],
  "max-len": ["error", 127],
  "require-jsdoc": ["off"],
  "space-infix-ops": ["error", {"int32Hint": false}]
}